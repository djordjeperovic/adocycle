{
  "name": "adocycle",
  "version": "0.1.7",
  "description": "Azure DevOps CLI for querying your assigned work items",
  "type": "module",
  "main": "./dist/cli.js",
  "bin": {
    "adocycle": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.20.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/cli.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "azure-devops",
    "cli",
    "typescript"
  ],
  "author": "Djordje Perovic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djordjeperovic/adocycle.git"
  },
  "bugs": {
    "url": "https://github.com/djordjeperovic/adocycle/issues"
  },
  "homepage": "https://github.com/djordjeperovic/adocycle#readme",
  "dependencies": {
    "@inquirer/prompts": "^8.3.0",
    "azure-devops-node-api": "^15.1.2",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.3",
    "env-paths": "^4.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.19.35",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
